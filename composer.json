{
    "name": "dmk/phpstan-rnbase",
    "description": "PHPStan extension for rn-base.",
    "type": "phpstan-extension",
    "require": {
        "phpstan/phpstan": "^0.11.8"
    },
    "require-dev": {
        "digedag/rn-base": "^1.9",
        "phpunit/phpunit": "^7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Bachmann",
            "email": "markus.bachmann@dmk-ebusiness.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DMK\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Type/data"
        ],
        "psr-4": {
            "DMK\\PHPStan\\Tests\\": "tests"
        }
    }
}
